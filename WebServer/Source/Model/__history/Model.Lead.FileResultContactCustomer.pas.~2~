Unit Model.Lead.FileResultContactCustomer;

Interface

Uses
    MVCFramework.ActiveRecord,
    MVCFramework.Nullables,
    System.Generics.Collections;

Type
    TProcessingStatus = (psUploaded, psParsing, psParsed, psFailed, psCompleted);

    [MVCNameCase(ncLowerCase)]
    [MVCTable('Lead_FileResultContactCustomer')]
    [MVCEntityActions([eaRetrieve, eaCreate, eaUpdate, eaDelete])]
    TLead_FileResultContactCustomer = Class(TMVCActiveRecord)
    Private
        FFileUploadID: Int64;
        FFileName: String;
        FUploadedByUserID: NullableInt32;
        FUploadedAt: TDateTime;
        FFileSize: NullableInt64;
        FErrorMessage: NullableString;

    Public
        [MVCTableField('FileUploadID', [foPrimaryKey, foAutoGenerated])]
        Property FileUploadID: Int64 Read FFileUploadID Write FFileUploadID;

        [MVCTableField('FileName')]
        Property FileName: String Read FFileName Write FFileName;

        [MVCTableField('UploadedByUserID')]
        Property UploadedByUserID: NullableInt32 Read FUploadedByUserID Write FUploadedByUserID;

        [MVCTableField('UploadedAt')]
        Property UploadedAt: TDateTime Read FUploadedAt Write FUploadedAt;

        [MVCTableField('FileSize')]
        Property FileSize: NullableInt64 Read FFileSize Write FFileSize;

        [MVCTableField('ErrorMessage')]
        Property ErrorMessage: NullableString Read FErrorMessage Write FErrorMessage;
    End;

Implementation

End.
