Unit Model.Lead.PhoneAssignment;

Interface

Uses
    MVCFramework.ActiveRecord,
    MVCFramework.Nullables,
    MVCFramework.Serializer.Commons,
    System.Generics.Collections;

Type
    [MVCNameCase(ncLowerCase)]
    [MVCTable('vw_phone_assignment')]
    [MVCEntityActions([eaRetrieve])]
    TPhoneAssignment = Class(TMVCActiveRecord)
    Private
        [MVCTableField('AssignmentID', [foPrimaryKey, foAutoGenerated])]
        FAssignmentID: Int64;

        [MVCTableField('Phone')]
        FPhone: String;

        [MVCTableField('BranchName')]
        FBranchName: String;

        [MVCTableField('BranchID')]
        FBranchID: Integer;

        [MVCTableField('SourceCollectingDataID')]
        FSourceCollectingDataID: Integer;

        [MVCTableField('SourceName')]
        FSourceName: String;

        [MVCTableField('UserName')]
        FUserName: String;
    Public
        Property AssignmentID: Int64 Read FAssignmentID Write FAssignmentID;
        Property Phone: String Read FPhone Write FPhone;
        Property BranchID: Integer Read FBranchID Write FBranchID;
        Property BranchName: String Read FBranchName Write FBranchName;
        Property SourceCollectingDataID: Integer Read FSourceCollectingDataID Write FSourceCollectingDataID;
        Property SourceName: String Read FSourceName Write FSourceName;
        Property UserName: String Read FUserName Write FUserName;
    End;

