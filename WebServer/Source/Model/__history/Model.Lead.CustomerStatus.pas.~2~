Unit Model.Lead.CustomerStatus;

Interface

Uses
    MVCFramework.ActiveRecord,
    MVCFramework.Nullables,
    System.Generics.Collections;

Type
    [MVCNameCase(ncLowerCase)]
    [MVCTable('lead_customerstatus')]
    [MVCEntityActions([eaRetrieve, eaCreate, eaUpdate, eaDelete])]
    TLead_CustomerStatus = Class(TMVCActiveRecord)
    Private
        FCustomerStatusID: Int32;
        FCode: NullableString;
        FName: NullableString;
        FIsActive: NullableBoolean;
        FIsSystemic: NullableBoolean;
        FDescription: NullableString;
        FCreatedAt: NullableDateTime;

    Public
        [MVCTableField('CustomerStatusID', [foPrimaryKey, foAutoGenerated])]
        Property CustomerStatusID: Int32 Read FCustomerStatusID Write FCustomerStatusID;

        [MVCTableField('Code')]
        Property Code: NullableString Read FCode Write FCode;

        [MVCTableField('Name')]
        Property Name: NullableString Read FName Write FName;

        [MVCTableField('IsActive')]
        Property IsActive: NullableBoolean Read FIsActive Write FIsActive;

        [MVCTableField('IsSystemic')]
        Property IsSystemic: NullableBoolean Read FIsSystemic Write FIsSystemic;

        [MVCTableField('Description')]
        Property Description: NullableString Read FDescription Write FDescription;

        [MVCTableField('CreatedAt')]
        Property CreatedAt: NullableDateTime Read FCreatedAt Write FCreatedAt;
    End;

Implementation

End.
