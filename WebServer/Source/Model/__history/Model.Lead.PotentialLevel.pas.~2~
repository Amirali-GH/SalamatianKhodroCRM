Unit Model.Lead.PotentialLevel;

Interface

Uses
    MVCFramework.ActiveRecord,
    MVCFramework.Nullables,
    System.Generics.Collections;

Type
    [MVCNameCase(ncLowerCase)]
    [MVCTable('Lead_PotentialLevel')]
    [MVCEntityActions([eaRetrieve, eaCreate, eaUpdate, eaDelete])]
    TLead_PotentialLevel = Class(TMVCActiveRecord)
    Private
        FPotentialLevelID: Int32;
        FCode: String;
        FName: String;
        FDescription: NullableString;
        FColor: NullableString;
        FPriority: NullableInt32;

    Public
        [MVCTableField('PotentialLevelID', [foPrimaryKey, foAutoGenerated])]
        Property PotentialLevelID: Int32 Read FPotentialLevelID Write FPotentialLevelID;

        [MVCTableField('Code')]
        Property Code: String Read FCode Write FCode;

        [MVCTableField('Name')]
        Property Name: String Read FName Write FName;

        [MVCTableField('Description')]
        Property Description: NullableString Read FDescription Write FDescription;

        [MVCTableField('Color')]
        Property Color: NullableString Read FColor Write FColor;

        [MVCTableField('Priority')]
        Property Priority: NullableInt32 Read FPriority Write FPriority;
    End;

Implementation

End.
