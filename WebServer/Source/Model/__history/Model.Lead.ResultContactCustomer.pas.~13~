Unit Model.Lead.ResultContactCustomer;

Interface

Uses
    MVCFramework.ActiveRecord,
    MVCFramework.Nullables,
    System.Generics.Collections;

Type
    [MVCNameCase(ncLowerCase)]
    [MVCTable('Lead_ResultContactCustomer')]
    [MVCEntityActions([eaRetrieve, eaCreate, eaUpdate, eaDelete])]
    TLead_ResultContactCustomer = Class(TMVCActiveRecord)
    Private
        [MVCTableField('ResultContactID', [foPrimaryKey, foAutoGenerated])]
        FResultContactID: Int64;

        [MVCTableField('CustomerID')]
        FCustomerID: Int64;

        FFielUploadID: NullableInt64;

        FLeadPotentialID: NullableInt32;

        FCampaignID: NullableInt32;

        FResultContactRefrenceID: NullableInt64;

        FBranchID: NullableInt32;

        FSaleAgentID: NullableInt32;

        FCustomerStatusID: NullableInt32;

        FRequestedCarID: NullableInt32;

        FRequestedCarName: NullableString;

        FLastContactDate_Shams: NullableString;

        FLastContactDate_Milad: NullableTDate;

        FNotes1: NullableString;

        FNotes2: NullableString;

        FNotes3: NullableString;

        [MVCTableField('CreatedAt', [foReadOnly])]
        FCreatedAt: NullableTDateTime;

    Public
        Property ResultContactID: Int64 Read FResultContactID Write FResultContactID;

        Property CustomerID: Int64 Read FCustomerID Write FCustomerID;

        [MVCTableField('FielUploadID')]
        Property FielUploadID: NullableInt64 Read FFielUploadID Write FFielUploadID;

        [MVCTableField('LeadPotentialID')]
        Property LeadPotentialID: NullableInt32 Read FLeadPotentialID Write FLeadPotentialID;

        [MVCTableField('CampaignID')]
        Property CampaignID: NullableInt32 Read FCampaignID Write FCampaignID;

        [MVCTableField('ResultContactRefrenceID')]
        Property ResultContactRefrenceID: NullableInt64 Read FResultContactRefrenceID Write FResultContactRefrenceID;

        [MVCTableField('BranchID')]
        Property BranchID: NullableInt32 Read FBranchID Write FBranchID;

        [MVCTableField('SaleAgentID')]
        Property SaleAgentID: NullableInt32 Read FSaleAgentID Write FSaleAgentID;

        [MVCTableField('CustomerStatusID')]
        Property CustomerStatusID: NullableInt32 Read FCustomerStatusID Write FCustomerStatusID;

        [MVCTableField('RequestedCarID')]
        Property RequestedCarID: NullableInt32 Read FRequestedCarID Write FRequestedCarID;

        [MVCTableField('RequestedCarName')]
        Property RequestedCarName: NullableString Read FRequestedCarName Write FRequestedCarName;

        [MVCTableField('LastContactDate_Shams')]
        Property LastContactDate_Shams: NullableString Read FLastContactDate_Shams Write FLastContactDate_Shams;

        [MVCTableField('LastContactDate_Milad')]
        Property LastContactDate_Milad: NullableTDate Read FLastContactDate_Milad Write FLastContactDate_Milad;

        [MVCTableField('Notes1')]
        Property Notes1: NullableString Read FNotes1 Write FNotes1;

        [MVCTableField('Notes2')]
        Property Notes2: NullableString Read FNotes2 Write FNotes2;

        [MVCTableField('Notes3')]
        Property Notes3: NullableString Read FNotes3 Write FNotes3;

        [MVCDoNotDeSerialize]
        Property CreatedAt: NullableTDateTime Read FCreatedAt Write FCreatedAt;
    End;

Implementation

End.
